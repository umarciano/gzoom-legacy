    <form name="WorkEffortViewSendMailManagementListForm" id="WESMML001_WorkEffortView" extends="BaseManagementDblClickListForm" extends-resource="component://base/widget/BaseForms.xml">
     	<row-actions>
     		<script location="component://workeffortext/webapp/workeffortext/WEB-INF/actions/getWorkEffortResponsibleByRole.groovy"/>
     	</row-actions>
        <field name="workEffortId">
            <hidden/>
        </field>
        <field name="orgUnitId" required-field="true">
            <display-entity entity-name="Party" description="${partyName}" key-field-name="partyId"/>
        </field>
        <field name="responsible">
            <display/>
        </field>
        <field name="responsibleId">
            <hidden/>
        </field>
        <field name="workEffortName">
            <display/>
        </field>
        <field name="currentStatusId" sort-field="true">
            <display-entity entity-name="StatusItem" key-field-name="statusId"/>
        </field>
        <field name="email" widget-area-style="emailIcon"><!-- width="30" height="30"  -->
        	 <image tooltip="${uiLabelMap.CommonSendEmail}" alternate="${uiLabelMap.CommonSendEmail}" ><!--  type="content" value="10000" -->
                <sub-hyperlink target="EditCommunicationEvent?externalLoginKey=${requestAttributes.externalLoginKey}&amp;partyIdFrom=${userLogin.partyId}&amp;partyIdTo=${responsibleId}&amp;workEffortId=${workEffortId}&amp;communicationEventTypeId=EMAIL_COMMUNICATION&amp;statusId=COM_IN_PROGRESS&amp;contactMechTypeId=EMAIL_ADDRESS&amp;communicationFormLocation=${communicationFormLocation}&amp;communicationFormName=${communicationFormName}&amp;lookupScreenLocation=component://base/widget/BaseScreens.xml&amp;actionMenuName=PopupMenuBar&amp;searchTabMenuName=MainPopupTabBar&amp;noConditionFind=Y&amp;entityName=Content&amp;contentTypeId=ICON">
                </sub-hyperlink>
            </image>
        </field>
    </form>
    
