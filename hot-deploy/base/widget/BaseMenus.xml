            <set field="infoTarget" value="${bsh: import org.ofbiz.base.util.UtilValidate; UtilValidate.isEmpty(parameters.get(&quot;userLoginId&quot;)) || !(parameters.get(&quot;userLoginId&quot;).equals(&quot;_NA_&quot;)) ? &quot;info&quot; : &quot;infoNotAuth&quot;}"/>
