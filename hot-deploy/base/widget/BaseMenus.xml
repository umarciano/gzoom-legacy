            <condition>
                <and>
                    <not><if-empty field="userLogin"/></not>
                    <if-compare field="ajaxRequest" operator="equals" value="Y"/>
                </and>
            </condition>
            <link id="back" target="${bsh: return !&quot;Y&quot;.equals(context.get(&quot;_SAVED_VIEW_NAME_&quot;)) ? &quot;&quot; : &quot;authBackLast&quot;}">
