        <!-- Sandro: sostituita azione home, con apertura/chiusura menu (vedi register-menu.js) -->
        <menu-item name="main">
            <condition>
                <not>
                    <if-empty field="__dummyFieldToIgnore"/> <!-- Per ignorare main, altrimenti mantiene la posizione sulla toolbar -->
