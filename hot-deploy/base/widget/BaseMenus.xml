            </condition>
            <link target="#"/>
        </menu-item>        
        <menu-item name="back" tooltip="${uiLabelMap.BaseMenusBackToPreviousPage}" align-style="back${bsh: return !&quot;Y&quot;.equals(context.get(&quot;_SAVED_VIEW_NAME_&quot;)) ? &quot;-disabled disabled&quot; : &quot;&quot;} action-menu-item fa fa-2x">
