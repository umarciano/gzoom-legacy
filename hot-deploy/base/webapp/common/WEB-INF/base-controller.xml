        <response name="error" type="request" value="main"/>
    </request-map>
    <request-map uri="setSessionOrganizzationId">
        <security https="true" auth="true"/>
        <event type="java" path="org.ofbiz.common.CommonEvents" invoke="setSessionOrganizzationId"/>
        <response name="success" type="request" value="main"/>
        <response name="error" type="request" value="main"/>
    </request-map>
    
    <request-map uri="getMessage">
