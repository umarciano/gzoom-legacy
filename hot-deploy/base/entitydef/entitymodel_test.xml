        <field name="date" type="date"></field>
        <field name="currency" type="currency-amount"></field>
        <field name="fixedpoint" type="fixed-point"></field>
        <field name="floatingpoint" type="floating-point"></field>
        
        <field name="datefrom" type="date-time"></field>
        <field name="datethru" type="date-time"></field>
        
        
        <prim-key field="testingId"/>
