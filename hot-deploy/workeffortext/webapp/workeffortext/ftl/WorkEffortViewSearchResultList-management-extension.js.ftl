WorkEffortViewSearchResultList = {
	
	load: function() {
	    WorkEffortViewSearchResultList.openLeftMenu();
	},
	
	openLeftMenu : function() {
        var mainContainer = $j("div.right-column");
        var leftCol = $j("div#left-bar-container");       
        mainContainer.removeClass("right-column-fullopen");
        leftCol.show();
        $j("li.open").show();
        $j("li.close").hide();
	}
}

WorkEffortViewSearchResultList.load();	
