                    <property name="position">3</property>
                    <property name="name">DESCRIZIONE_OBIETTIVO</property>
                    <property name="nativeName">DESCRIZIONE_OBIETTIVO</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">GL_ACCOUNT_FIN_ID</property>
                    <property name="nativeName">GL_ACCOUNT_FIN_ID</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">DATA_TRANSAZIONE</property>
                    <property name="nativeName">DATA_TRANSAZIONE</property>
                    <property name="dataType">date-time</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">BUDGET</property>
                    <property name="nativeName">BUDGET</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">EXTRA_BUDGET</property>
                    <property name="nativeName">EXTRA_BUDGET</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">COMMENTO_BDG</property>
                    <property name="nativeName">COMMENTO_BDG</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">9</property>
                    <property name="name">RIFERIMENTO_BDG</property>
                    <property name="nativeName">RIFERIMENTO_BDG</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">10</property>
                    <property name="name">COMMENTO_EXTRA</property>
                    <property name="nativeName">COMMENTO_EXTRA</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">11</property>
                    <property name="name">RIFERIMENTO_EXTRA</property>
                    <property name="nativeName">RIFERIMENTO_EXTRA</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">12</property>
                    <property name="name">STATO_SCHEDE</property>
                    <property name="nativeName">STATO_SCHEDE</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">13</property>
                    <property name="name">DESCRIZIONE_STATO_SCHEDE</property>
                    <property name="nativeName">DESCRIZIONE_STATO_SCHEDE</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT RT.GL_ACCOUNT_FIN_ID AS ID_CAPITOLO,
       OB.SOURCE_REFERENCE_ID AS CODICE_OBIETTIVO, 
       OB.WORK_EFFORT_NAME    AS DESCRIZIONE_OBIETTIVO, 
       RT.GL_ACCOUNT_FIN_ID, TT.TRANSACTION_DATE AS DATA_TRANSAZIONE,       
  SUM (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'STANZIATO'   THEN RT.AMOUNT ELSE 0 END) AS BUDGET , 
  SUM (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' THEN RT.AMOUNT ELSE 0 END) AS EXTRA_BUDGET ,
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'STANZIATO' THEN    TT.DESCRIPTION END) AS COMMENTO_BDG,
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'STANZIATO' THEN    RT.DESCRIPTION END) AS RIFERIMENTO_BDG,
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' THEN  TT.DESCRIPTION END) AS COMMENTO_EXTRA,
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' THEN  RT.DESCRIPTION END) AS RIFERIMENTO_EXTRA,
-- AGGIUNTO STATO SCHEDE - INIZIO
    TT.GROUP_STATUS_ID AS STATO_SCHEDE,
    ST.DESCRIPTION AS DESCRIZIONE_STATO_SCHEDE
-- AGGIUNTO STATO SCHEDE - FINE
FROM ACCTG_TRANS_ENTRY  RT
JOIN ACCTG_TRANS TT ON TT.ACCTG_TRANS_ID = RT.ACCTG_TRANS_ID
    AND (RT.GL_FISCAL_TYPE_ID = 'STANZIATO' OR RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' )
-- AGGIUNTO STATO SCHEDE - INIZIO
JOIN STATUS_ITEM ST ON TT.GROUP_STATUS_ID = ST.STATUS_ID
-- AGGIUNTO STATO SCHEDE - FINE
JOIN WORK_EFFORT_MEASURE M ON M.WORK_EFFORT_MEASURE_ID = RT.VOUCHER_REF
JOIN WORK_EFFORT OB ON OB.WORK_EFFORT_ID = M.WORK_EFFORT_ID

WHERE GL_ACCOUNT_FIN_ID = ?    -- ID  CAPITOLO
		AND TT.TRANSACTION_DATE = ?
  GROUP BY RT.GL_ACCOUNT_FIN_ID, OB.SOURCE_REFERENCE_ID,OB.WORK_EFFORT_NAME , TT.TRANSACTION_DATE, 
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'STANZIATO' THEN    TT.DESCRIPTION END),
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'STANZIATO' THEN    RT.DESCRIPTION END),
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' THEN  TT.DESCRIPTION END),
    (CASE WHEN RT.GL_FISCAL_TYPE_ID = 'EXTRABUDGET' THEN  RT.DESCRIPTION END),
    -- AGGIUNTO STATO SCHEDE - INIZIO
    TT.GROUP_STATUS_ID ,  ST.DESCRIPTION     
    -- AGGIUNTO STATO SCHEDE - FINE
ORDER BY RT.GL_ACCOUNT_FIN_ID, OB.SOURCE_REFERENCE_ID
]]></xml-property>
        </oda-data-set>
    </data-sets>
