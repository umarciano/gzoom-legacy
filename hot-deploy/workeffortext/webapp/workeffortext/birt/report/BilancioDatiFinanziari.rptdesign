            <xml-property name="queryText"><![CDATA[SELECT WTY.WORK_EFFORT_TYPE_ID AS CODICE_FASE,
       WTY.DESCRIPTION         AS DES_FASE,
       P_G_C.PARENT_ROLE_CODE  AS CODICE_CENTRO_GES,
       P_G.PARTY_NAME          AS DES_CENTRO_GES,
       R_CDG.PARTY_NAME        AS DES_RESPONSABILE_CENTRO_GES,
        (CASE WHEN G.DEBIT_CREDIT_DEFAULT = 'C' THEN 'ENTRATA' ELSE 'SPESA' END )  AS ENTRATA_SPESA,
      P_R_C.PARENT_ROLE_CODE  AS CODICE_CENTRO_RESP,
       P_R.PARTY_NAME          AS DES_CENTRO_RESP,
       R_CDR.PARTY_NAME        AS DES_RESPONSABILE_CENTRO_RESP,
       MOV.BUDGET_RAGIONERIA,
       G.ACCOUNT_CODE AS CODICE_CAPITOLO, 
       G.ACCOUNT_NAME AS DESCRIZIONE_CAPITOLO   ,
       G.GL_ACCOUNT_ID AS ID_CAPITOLO,
       CP.THRU_DATE AS DATA_PERIODO,
       MOV_CAP.BUDGET_TOTALE_CAPITOLO  -- utilizzato nella funzione "bilancio definitivo"  (
FROM GL_ACCOUNT G
JOIN WORK_EFFORT_PURPOSE_ACCOUNT P ON G.GL_ACCOUNT_ID = P.GL_ACCOUNT_ID
      AND P.WORK_EFFORT_PURPOSE_TYPE_ID = 'RESOURCE'  -- SELEZIONA SOLO CAPITOLI VALIDI
JOIN PARTY  P_R ON P_R.PARTY_ID = G.RESP_CENTER_ID
JOIN PARTY_PARENT_ROLE  P_R_C ON P_R_C.PARTY_ID = G.RESP_CENTER_ID
      AND P_R_C.ROLE_TYPE_ID = 'ORGANIZATION_UNIT'
JOIN GL_ACCOUNT_ROLE G_R ON G_R.GL_ACCOUNT_ID = G.GL_ACCOUNT_ID
JOIN PARTY  P_G ON P_G.PARTY_ID = G_R.PARTY_ID
JOIN PARTY_PARENT_ROLE  P_G_C ON P_G.PARTY_ID = P_G_C.PARTY_ID
      AND P_G_C.ROLE_TYPE_ID = 'ORGANIZATION_UNIT'
JOIN WORK_EFFORT_TYPE_PERIOD WT ON WT.WORK_EFFORT_TYPE_ID = WT.WORK_EFFORT_TYPE_ID
      AND WT.GL_FISCAL_TYPE_ENUM_ID = 'GLFISCTYPE_TARGETFIN'
JOIN WORK_EFFORT_TYPE WTY ON WT.WORK_EFFORT_TYPE_ID = WTY.WORK_EFFORT_TYPE_ID
JOIN CUSTOM_TIME_PERIOD CP ON WT.custom_time_period_id = cp.custom_time_period_id
      AND ( G.THRU_DATE >= CP.THRU_DATE OR G.THRU_DATE IS NULL) -- LA DATA FINE   SU CESENA ATTUALMENTE E' VALORIZZATA SOLO QUANDO CESSATO
      AND ( G.FROM_DATE <= CP.THRU_DATE OR G.FROM_DATE IS NULL) -- LA DATA INIZIO SU CESENA ATTUALMENTE NON E' MAIL VALORIZZATA
LEFT OUTER JOIN PARTY_RELATIONSHIP PR_CDR ON PR_CDR.PARTY_ID_FROM = P_R.PARTY_ID
      AND PR_CDR.PARTY_RELATIONSHIP_TYPE_ID = 'ORG_RESPONSIBLE'
      AND PR_CDR.THRU_DATE IS NULL
LEFT OUTER JOIN PARTY  R_CDR ON PR_CDR.PARTY_ID_TO = R_CDR.PARTY_ID
LEFT OUTER JOIN PARTY_RELATIONSHIP PR_CDG ON PR_CDG.PARTY_ID_FROM = P_G.PARTY_ID
      AND PR_CDG.PARTY_RELATIONSHIP_TYPE_ID = 'ORG_RESPONSIBLE'
      AND PR_CDG.THRU_DATE IS NULL
LEFT OUTER JOIN PARTY  R_CDG ON PR_CDG.PARTY_ID_TO = R_CDG.PARTY_ID
LEFT JOIN (SELECT RT.PARTY_ID AS PARTY_ID_CDC,TT.PARTY_ID AS PARTY_ID_CDR,TT.TRANSACTION_DATE,
           (CASE WHEN RT.GL_ACCOUNT_ID = 'CDC_SPESA' THEN 'D'  ELSE 'C'  END ) AS DEBIT_CREDIT,
           SUM(RT.AMOUNT)   AS BUDGET_RAGIONERIA 
           FROM ACCTG_TRANS_ENTRY  RT
           JOIN ACCTG_TRANS TT ON TT.ACCTG_TRANS_ID = RT.ACCTG_TRANS_ID
           WHERE (RT.GL_ACCOUNT_ID = 'CDC_ENTRATA' OR RT.GL_ACCOUNT_ID = 'CDC_SPESA')
                  AND RT.GL_FISCAL_TYPE_ID = 'STANZIATO'
           GROUP BY RT.PARTY_ID,TT.PARTY_ID, (CASE WHEN RT.GL_ACCOUNT_ID = 'CDC_SPESA' THEN 'D'  ELSE 'C'  END ),TT.TRANSACTION_DATE) MOV
      ON MOV.TRANSACTION_DATE = CP.THRU_DATE
      AND MOV.PARTY_ID_CDR  = P_R.PARTY_ID
      AND MOV.PARTY_ID_CDC  = P_G.PARTY_ID
      AND  MOV.DEBIT_CREDIT  = G.DEBIT_CREDIT_DEFAULT       
LEFT JOIN (SELECT RT.PARTY_ID AS PARTY_ID_CDC,
                  TT.PARTY_ID AS PARTY_ID_CDR,
                  TT.TRANSACTION_DATE,GL_ACCOUNT_ID,
                  SUM(RT.AMOUNT)   AS BUDGET_TOTALE_CAPITOLO
           FROM ACCTG_TRANS_ENTRY  RT
           JOIN ACCTG_TRANS TT ON TT.ACCTG_TRANS_ID = RT.ACCTG_TRANS_ID
           WHERE (RT.GL_ACCOUNT_ID <>  'CDC_ENTRATA' AND RT.GL_ACCOUNT_ID <>  'CDC_SPESA')
                 AND RT.GL_FISCAL_TYPE_ID = 'STANZIATO'
                 AND RT.VOUCHER_REF IS NULL
          GROUP BY RT.PARTY_ID ,TT.PARTY_ID,TT.TRANSACTION_DATE,GL_ACCOUNT_ID ) MOV_CAP
      ON MOV_CAP.TRANSACTION_DATE = CP.THRU_DATE
      AND MOV_CAP.PARTY_ID_CDR  = P_R.PARTY_ID
      AND MOV_CAP.PARTY_ID_CDC = P_G.PARTY_ID
      AND MOV_CAP.GL_ACCOUNT_ID = G.GL_ACCOUNT_ID --- VERIFICARE SE FARE LINK SO CAMPO DATI FINANZIARI .....
      
WHERE      G.ACCOUNT_TYPE_ENUM_ID = 'FINANCIAL'  
  AND WT.WORK_EFFORT_TYPE_ID = ? -- PARAMETRO MAPPA FASE  (PREVISTI ANCHE ALTRI 2 PARAMETRI....)
  AND P_G.PARTY_ID = ?   -- FILTRO CENTRO GESTORE  (FILTRO) '  (esempio "Centro elaborazione dati ")
ORDER BY   P_G_C.PARENT_ROLE_CODE, (CASE WHEN DEBIT_CREDIT_DEFAULT = 'C' THEN 'ENTRATA' ELSE 'SPESA' END ) DESC, P_R_C.PARENT_ROLE_CODE,-- PRIMA SPESE E POI ENTRATE
G.ACCOUNT_CODE       ]]></xml-property>
        </oda-data-set>
