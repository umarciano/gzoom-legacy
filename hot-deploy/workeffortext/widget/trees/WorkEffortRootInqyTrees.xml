<?xml version="1.0" encoding="UTF-8"?>

<trees xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-tree.xsd">

    <!-- Nome dell'albero - riferimento alla colonna principale - Visualizza inizio, chiuso:0, aperto: 1 -->
    <tree name="WorkEffortRootInqyTree" root-node-name="wrToName" open-depth="0">
        <node name="wrToName">
            <label text="${uiLabelMap.WorkeffortRoot}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="wrToName"/>
            </sub-node>
        </node>
        <node name="wrToCode">
            <label text="${uiLabelMap.FormFieldTitle_sourceReferenceId}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="wrToCode"/>
            </sub-node>
        </node>
        <node name="assocWeight">
            <label text="${uiLabelMap.FormFieldTitle_assocWeight}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="assocWeight"/>
            </sub-node>
        </node>
        <node name="etch">
            <label text="${uiLabelMap.CommonType}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="etch"/>
            </sub-node>
        </node>
    </tree>
    
    <!-- nome albero nel caso bilinguismo -->
    <tree name="WorkEffortRootInqyTreeLang" root-node-name="wrToNameLang" open-depth="0">
        <node name="wrToNameLang">
