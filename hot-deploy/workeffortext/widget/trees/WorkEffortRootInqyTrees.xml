        <node name="wrToCode">
            <label text="${uiLabelMap.FormFieldTitle_sourceReferenceId}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="wrToCode"/>
            </sub-node>
        </node>
        <node name="assocWeight">
            <label text="${uiLabelMap.FormFieldTitle_assocWeight}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="assocWeight"/>
            </sub-node>
        </node>
        <node name="etch">
            <label text="${uiLabelMap.CommonType}"/>
            <sub-node node-name=" ">
                <out-field-map field-name="etch"/>
            </sub-node>
        </node>
    </tree>

</trees>
