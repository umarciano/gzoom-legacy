<?xml version="1.0" encoding="UTF-8"?>

<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

	<form name="AccountingExtStandardImportForm" id="ASTD001_StandardImport" type="upload" target-window="target_upload" default-table-style="single-editable" style="uploadFile basic-form cachable content-upload-form" target="executeStandardImportUploadFile">
		<field name="defaultOrganizationPartyId">
		    <hidden/>
		</field>
		<field name="acctgTransInterface" title="${uiLabelMap.AcctgTransInterface}">
			<display/>
		</field>
		<field name="entityListToImportAcctgTransInterface" widget-style="execute-field" position="2"  title=" ">
        	<check all-checked="false">
        		<option key="AcctgTransInterface" description=" "/>
        	</check>
        </field>        
        <field name="AcctgTransInterfaceRefDate" widget-style="options-field" position="3" title=" "  widget-area-style="columnWidthClass15">
            <date-time type="date"/>
        </field>
        <field name="AcctgTransInterfaceDataSourceId" widget-style="options-field" position="4" title=" " widget-area-style="columnWidthClass25">
            <drop-down type="drop-list" maxlength="255" size="30" local-autocompleter="true" drop-list-key-field="dataSourceId" drop-list-display-field="description">
                <entity-options entity-name="DataSource" key-field-name="dataSourceId" description="${description}">
                    <select-field field-name="dataSourceId"/>
                    <select-field field-name="description" display="true"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="AcctgTransInterfaceUploadedFile" widget-area-style="${bsh: context.get(&quot;AcctgTransInterfaceListSizeLabel&quot;)} ? &quot;columnWidthClass20&quot; : &quot;&quot;}" position="5" title=" ">
			<file size="50" maxlength="255"/>
		</field>
        <field name="AcctgTransInterfaceListSizeLabel" widget-style="red" position="6"  title=" ">
            <display/>
        </field>
		
		<field name="glAccountInterface" title="${uiLabelMap.GlAccountInterface}" >
			<display/>
		</field>
        <field name="entityListToImportGlAccountInterface" widget-style="execute-field" position="2" title=" ">
        	<check all-checked="false">
        		<option key="GlAccountInterface" description=" "/>
        	</check>
        </field>
        <field name="GlAccountInterfaceRefDate" widget-style="options-field" position="3" title=" "  widget-area-style="columnWidthClass15">
            <date-time type="date"/>
        </field>
        <field name="GlAccountInterfaceDataSourceId" widget-style="options-field" position="4" title=" " widget-area-style="columnWidthClass25">
            <drop-down type="drop-list" maxlength="255" size="30" local-autocompleter="true" drop-list-key-field="dataSourceId" drop-list-display-field="description">
                <entity-options entity-name="DataSource" key-field-name="dataSourceId" description="${description}">
                    <select-field field-name="dataSourceId"/>
                    <select-field field-name="description" display="true"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="GlAccountInterfaceUploadedFile" widget-area-style="${bsh: context.get(&quot;GlAccountInterfaceListSizeLabel&quot;)} ? &quot;columnWidthClass20&quot; : &quot;&quot;}" position="5" title=" ">
			<file size="50" maxlength="255"/>
		</field>
        <field name="GlAccountInterfaceListSizeLabel" widget-style="red" position="6"  title=" ">
            <display/>
        </field>
		
		<field name="entityListToImport" widget-style="execute-field-name">
        	<hidden/>
        </field>
        <field name="checkOnlyUploadTitle" title="${uiLabelMap.StandardImportUpload}" >
            <display/>
        </field>
        <field widget-style="options-field" position="2"  name="checkOnlyUpload" title=" ">
            <check>
                <option key="onlyUpload" description=" "/>
            </check>
        </field>
    </form>
</forms>