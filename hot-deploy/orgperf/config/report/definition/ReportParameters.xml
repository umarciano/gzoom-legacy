<?xml version="1.0" encoding="UTF-8" ?>
<reports xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/report-parameters.xsd">
    <report id="REPORT_SRI" name="StampaImpiegoRisorse" description="Stampa Impiego Risorse">
	    <parameter name="requireTransaction" default-value="Y"/>
    	<parameter name="reportContentId"/>
	    <parameter name="orgUnitRoleTypeId"/>
	    <parameter name="orgUnitId"/>
	    <parameter name="employeeId"/>	   
	    <parameter name="thruDate" type="Timestamp">
        	<event type="java" path="com.mapsengineering.workeffortext.birt.event.WorkEffortextBirtEvents" invoke="setDefaultThruDate"/>
        </parameter> 
	    <parameter name="fromDate" type="Timestamp" mandatory="true" validation-message="BaseErrorLabels#CommonThruDateNotNullValidation"/>
	    <parameter name="roleTypeWeight" default-value="N"/>
	    <parameter name="selectionSection" default-value="ALL"/>
	    <parameter name="outputFormat"/>
    </report>
    <report id="REPORT_SIP" name="StampaImpieghiPeriodo" description="Stampa Scheda Impieghi di Periodo">
	    <parameter name="reportContentId"/>
	    <parameter name="orgUnitRoleTypeId"/>
	    <parameter name="orgUnitId"/>
	    <parameter name="employeeId"/>	    
	    <parameter name="fromDate" type="Timestamp" mandatory="true" validation-message="BaseErrorLabels#CommonFromDateNotNullValidation"/>
	    <parameter name="thruDate" type="Timestamp" mandatory="true" validation-message="BaseErrorLabels#CommonThruDateNotNullValidation"/>
	    <parameter name="roleTypeWeight" default-value="N"/>
	    <parameter name="selectionSection" default-value="ALL"/>
	    <parameter name="outputFormat"/>
    </report>
    
    <report id="REPORT_SPEG" name="SchedaPEG" description="Stampa piano esecutivo di gestione">
	    <parameter name="workEffortId" />
	    <parameter name="workEffortIdChild">
           <event type="java" path="com.mapsengineering.workeffortext.birt.event.WorkEffortextBirtEvents" invoke="checkChildRootEqualityWithoutOrgUnit"/>
        </parameter>
	    <parameter name="orgUnitId"/>
	    <parameter name="roleTypeId"/>
	    <parameter name="partyId"/>
	    <parameter name="personId"/>
	    <parameter name="workEffortTypeIdRef"/>
	    <parameter name="monitoringDate" type="Timestamp"  mandatory="true" validation-message="BaseErrorLabels#MonitoringDateNotNullValidation"/>
	    <parameter name="reportContentId" />
	    <parameter name="assessors"/>	
	    <!-- PARAMETRI OPTION   -->
	    <parameter name="exposePaginator" default-value="Y"/>
	    <parameter name="exposeReleaseDate" default-value="Y"/>
	    <parameter name="outputFormat"/>
	    <parameter name="workEffortTypeId"/>
    </report>
</reports>