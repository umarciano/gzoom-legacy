<?xml version="1.0" encoding="UTF-8"?>

<menus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-menu.xsd">

    <menu name="JobLogManagementTabMenu" id="JobLogManagementTabMenu" title="" extends="MainCommonTabBar" extends-resource="component://base/widget/BaseMenus.xml">
        <menu-item name="JobLog" tooltip="${uiLabelMap.JobLogTab}" title="${uiLabelMap.JobLogTab}">
            <condition>
                <not>
                    <if-empty field="userLogin"/>
                </not>
            </condition>
            <link target="#management_0" url-mode="plain"/>
        </menu-item>
        <menu-item name="JobLogJobExecParams" tooltip="${uiLabelMap.JobLogJobExecParamsTab}" title="${uiLabelMap.JobLogJobExecParamsTab}">
            <condition>
                <and>
                    <not>
                        <if-empty field="userLogin"/>
                    </not>
                    <if-compare operator="equals" value="N" field="insertMode"/>
                </and>
            </condition>
            <link target="#management_1" url-mode="plain"/>
        </menu-item>
        <menu-item name="JobLogLog" tooltip="${uiLabelMap.JobLogLogTab}" title="${uiLabelMap.JobLogLogTab}">
            <condition>
                <and>
                    <not>
                        <if-empty field="userLogin"/>
                    </not>
                    <if-compare operator="equals" value="N" field="insertMode"/>
                </and>
            </condition>
            <link target="#management_2" url-mode="plain"/>
        </menu-item>
    </menu>
    
</menus>
