<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Services</description>
    <vendor>GPlus</vendor>
    <version>1.0</version>

    <!--
        ****************
        INTERFACES
        ****************
    -->
    <service name="purgeJobs" auth="true" use-transaction="false" engine="groovy" location="component://base/script/com/mapsengineering/base/purgeJobs.groovy">
    </service>
    <!--
        Interfaccia per servizi custom salvataggio D&D su treeview
    -->
    <service name="saveTreeViewChangesInterface" engine="interface">
        <description>Save treeview changes</description>
        <attribute name="entityName" mode="IN" type="String" optional="false"/>
        <attribute name="parentKeys" mode="IN" type="Map" optional="false"/>
        <attribute name="originalKeys" mode="IN" type="Map" optional="false"/>
        <attribute name="relationKeys" mode="IN" type="List" optional="false"/>
        <attribute name="defaultValueMap" mode="IN" type="Map" optional="true"/>
        <!-- Id's from create operation -->
        <attribute name="id" mode="OUT" type="Map" optional="true"/>
        <!-- Optionally return values -->
        <attribute name="retValues" mode="OUT" type="List" optional="true"/>
    </service>

    <!--
        Interfaccia per i servizi che necessitano di log
        Definisce solo i parametri comuni di output per la scrittura del job log
    -->
    <service name="loggedServiceInterface" engine="interface">
        <description>loggedServiceInterface</description>
        <!-- Parametri output per Job Log -->
        <attribute name="runResults" mode="OUT" type="List" optional="false"/>
        <attribute name="recordElaborated" mode="OUT" type="Long" optional="false"/>
        <attribute name="errorMessages" mode="OUT" type="Long" optional="false"/>
        <attribute name="warnMessages" mode="OUT" type="Long" optional="false"/>
        <!-- Eventuale chiavi passate nei parametri al servizio e che vengono scritte in JobLog per facilitare la ricerca -->
        <attribute name="elabRef1" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="baseImportFromInterface" engine="interface" auth="true">
        <attribute name="filePath" mode="IN" type="String" optional="false"/>
        <attribute name="recordElaborated" mode="OUT" type="Integer" optional="false"/>
    </service>

    <!-- ********************************************************************** -->
    <!-- Install Services -->

    <service name="runOfbizContainer" use-transaction="false" engine="simple" invoke="runOfbizContainer" location="com/mapsengineering/base/base-services.xml">
        <attribute name="containerClassName" mode="IN" type="String"/>
        <attribute name="args" mode="IN" type="List" optional="true"/>
        <attribute name="configFile" mode="IN" type="String" optional="true"/>
    </service>

    <service name="installFile" use-transaction="false" engine="simple" invoke="installFile" location="com/mapsengineering/base/base-services.xml">
        <attribute name="dataFile" mode="IN" type="String" optional="true"/>
    </service>

    <service name="installReaders" use-transaction="false" engine="simple" invoke="installReaders" location="com/mapsengineering/base/base-services.xml">
        <attribute name="dataReaders" mode="IN" type="String" optional="true"/>
    </service>

    <service name="installCustomSeed" use-transaction="false" engine="simple" invoke="installCustomSeed" location="com/mapsengineering/base/base-services.xml">
    </service>

    <service name="installCustomSeedInitial" use-transaction="false" engine="simple" invoke="installCustomSeedInitial" location="com/mapsengineering/base/base-services.xml">
    </service>

    <service name="julReadConfiguration" engine="java" location="com.mapsengineering.base.services.LoggingServices" invoke="julReadConfigurationSrv" use-transaction="false">
    </service>

    <!--
        ****************
        SERVICES
        ****************
    -->

    <service name="gzSleep" engine="simple" invoke="gzSleep" location="com/mapsengineering/base/base-services.xml" auth="false" use-transaction="false">
        <attribute name="millis" mode="IN" type="Long" optional="true"/>
    </service>

    <service name="getMessage" engine="simple" invoke="getMessage" location="com/mapsengineering/base/base-services.xml" auth="false">
        <description>Get message from uiLabelMap</description>
        <attribute name="resource" mode="IN" type="String" optional="true" default-value="BaseUiLabels"/>
        <attribute name="property" mode="IN" type="String" optional="false"/>
        <attribute name="value" mode="OUT" type="String" optional="true"/>
    </service>

    <service name="fieldValidation" engine="java" location="com.mapsengineering.base.services.ClientValidationServices" invoke="doFieldValidation" auth="true">
        <description>Form field validation</description>
        <attribute name="formName" mode="IN" type="String" optional="false"/>
        <attribute name="fieldName" mode="IN" type="String" optional="false"/>
        <attribute name="fieldValue" mode="IN" type="String" optional="false"/>
        <attribute name="validator" mode="IN" type="String" optional="true"/>
        <attribute name="decimalDigits" mode="IN" type="String" optional="true"/>
    </service>

    <!-- Default service save treeview D&D changes -->
    <service name="saveTreeViewChanges" engine="simple" invoke="saveTreeViewChanges" location="com/mapsengineering/base/base-services.xml" auth="true">
        <implements service="saveTreeViewChangesInterface"/>
    </service>

    <!-- Default service save treemenu preferred -->
    <service name="saveTreeMenuPreferred" engine="simple" invoke="saveTreeMenuPreferred" location="com/mapsengineering/base/base-services.xml" auth="true">
        <description>Default service save treemenu preferred</description>
        <attribute name="preferences" mode="IN" type="String" optional="false"/>
    </service>

    <service name="getTreeMenuPreferred" engine="simple" invoke="getTreeMenuPreferred" location="com/mapsengineering/base/base-services.xml" auth="true">
        <description>Default service get treemenu preferred</description>
        <attribute name="preferences" mode="OUT" type="String" optional="true"/>
    </service>

    <!--
        Generic Service Wrapper
        Log services into JobLog entity
    -->
    <service name="serviceLogger" engine="java" invoke="runService" location="com.mapsengineering.base.services.ServiceLogger" use-transaction="false">
        <implements service="loggedServiceInterface"/>
        <!-- Nome del servizio da lanciare -->
        <attribute name="serviceName" mode="IN" type="String" optional="false"/>
        <!-- Parametri per il servizio -->
        <attribute name="serviceParameters" mode="IN" type="Map" optional="false"/>
        <!-- Type Id del servizio, se passato, deve esistere nella entity JobLogServiceType -->
        <attribute name="serviceTypeId" mode="IN" type="String" optional="true"/>
        <!-- Timeout della transazione interna nella quale viene richiamato il servizio (Default e' quello del servizio chiamato (serviceName) ) -->
        <attribute name="requireNewTransaction" mode="IN" type="Boolean" optional="true" default-value="false"/>
        <attribute name="transactionTimeout" mode="IN" type="Integer" optional="true" default-value="-1"/>
        <attribute name="sessionId" mode="IN" type="String" optional="true"/>
        <attribute name="jobLogId" mode="OUT" type="String" optional="true"/>
        <attribute name="warningMessages" mode="OUT" type="Long" optional="true"/>
        <attribute name="warnMessages" mode="OUT" type="Long" optional="true"/>
        <attribute name="blockingErrors" mode="OUT" type="Long" optional="true"/>
        <attribute name="runResults" mode="OUT" type="List" optional="true"/>
        <attribute name="errorMessages" mode="OUT" type="Long" optional="true"/>
        <!-- Parametri in uscita -->
        <attribute name="serviceOutParameters" mode="OUT" type="Map" optional="true"/>
    </service>

    <!-- -->

    <service name="ofbizLogAnalysis" use-transaction="false" engine="groovy" location="component://base/script/com/mapsengineering/base/loganalysis/ofbizLogAnalysis.groovy">
        <attribute name="logFileLocation" mode="IN" type="String" optional="true"/>
    </service>

    <!-- -->

    <!--
        ****************
        Help services
        ****************
    -->

    <!--
        Get context Help
    -->
    <service name="getContextHelp" engine="java" location="com.mapsengineering.base.services.HelpServices" invoke="getContextHelp" auth="true">
        <description>Get context help for form, entity, field</description>
        <attribute name="formName" mode="IN" type="String" optional="true"/>
        <attribute name="entityName" mode="IN" type="String" optional="false"/>
        <attribute name="fieldName" mode="IN" type="String" optional="false"/>
        <attribute name="accurate" mode="IN" type="Boolean" optional="false"/>
        <attribute name="helpText" mode="INOUT" type="String" optional="true"/>
    </service>

    <!--
        Create context Help
    -->
    <service name="createContextHelp" engine="java" location="com.mapsengineering.base.services.HelpServices" invoke="createContextHelp" auth="true">
        <description>Get context help for form, entity, field</description>
        <attribute name="formName" mode="IN" type="String" optional="true"/>
        <attribute name="entityName" mode="IN" type="String" optional="false"/>
        <attribute name="fieldName" mode="IN" type="String" optional="false"/>
        <attribute name="helpText" mode="IN" type="String" optional="false"/>
    </service>

    <service name="dateValidationService" engine="simple" invoke="dateValidationService" location="com/mapsengineering/base/base-services.xml" auth="true">
        <description>Checks if fromDate is before thruDate</description>
        <attribute name="fromDate" mode="IN" type="Timestamp" optional="false"/>
        <attribute name="thruDate" mode="IN" type="Timestamp" optional="true"/>
    </service>

    <service name="uploadContentFile_DateValidation" engine="simple" invoke="uploadContentFile_DateValidation" location="com/mapsengineering/base/base-services.xml" auth="true">
        <description>Checks if fromDate is before thruDate. Specific for upload forms</description>
        <attribute name="fromDate" mode="IN" type="Timestamp" optional="false"/>
        <attribute name="thruDate" mode="IN" type="Timestamp" optional="true"/>
    </service>

    <service name="updatePasswordExt" engine="simple" invoke="updatePasswordExt" location="com/mapsengineering/base/base-services.xml" auth="true">
        <description>Wraps Ofbiz updatePassword service by making mandatory the password fields</description>
        <implements service="updatePassword"/>
        <attribute name="messageContext" mode="OUT" type="String" optional="true"/>
        <override name="userLoginId" mode="IN" type="String" optional="false"/>
        <override name="currentPassword" mode="IN" type="String" optional="false"/>
        <override name="newPassword" mode="IN" type="String" optional="false"/>
        <override name="newPasswordVerify" mode="IN" type="String" optional="false"/>
    </service>

    <service name="standardImport" engine="java" location="com.mapsengineering.base.standardimport.ImportManager" invoke="doImportSrv" auth="true" use-transaction="false">
        <attribute name="entityListToImport" mode="IN" type="String" optional="false"/>
        <attribute name="filterMapList" mode="IN" type="List" optional="true"/>
        <attribute name="filterConditions" mode="IN" type="List" optional="true"/>
