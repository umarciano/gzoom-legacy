    <!-- SERVIZIO ESECUTORE QUERY -->
    <service name="queryExecutorService" engine="java" location="com.mapsengineering.base.services.QueryExecutorService" invoke="runExecuteQuery" auth="true" use-transaction="false" transaction-timeout="7200"  max-retry="0">
        <attribute name="request" type="javax.servlet.http.HttpServletRequest" mode="IN" optional="true"/>
        <attribute name="response" mode="IN" type="javax.servlet.http.HttpServletResponse" optional="true"/>
        <attribute name="queryId" mode="IN" type="String" optional="true" />
        <attribute name="cond0Info" mode="IN" type="String" optional="true" />
        <attribute name="cond1Info" mode="IN" type="String" optional="true" />
        <attribute name="cond2Info" mode="IN" type="String" optional="true" />
        <attribute name="cond3Info" mode="IN" type="String" optional="true" />
        <attribute name="cond4Info" mode="IN" type="String" optional="true" />
        <attribute name="cond5Info" mode="IN" type="String" optional="true" />
        <attribute name="cond6Info" mode="IN" type="String" optional="true" />
        <attribute name="cond7Info" mode="IN" type="String" optional="true" />
        <attribute name="cond8Info" mode="IN" type="String" optional="true" />
        <attribute name="jobLogger" mode="INOUT" type="com.mapsengineering.base.util.JobLogger" optional="true"/>
        <attribute name="recordElaborated" mode="OUT" type="Long" optional="true"/>
        <attribute name="blockingErrors" mode="OUT" type="Long" optional="true"/>
    </service>
    
