        <attribute name="checkOnlyUpload" mode="IN" type="String" optional="true"/>
        <attribute name="deletePrevious" mode="IN" type="String" optional="true"/>
        <attribute name="sessionId" mode="IN" type="String" optional="true"/>
        <attribute name="jobLogId" mode="IN" type="String" optional="true"/>
		<attribute name="checkEndYearElab" mode="IN" type="String" optional="true"/>
        <!-- Poiche servizio non schedulato, prende dal form il defaultOrganizationPartyId -->
        <attribute name="defaultOrganizationPartyId" mode="IN" type="String" optional="true"/>
        <attribute name="resultETLList" mode="OUT" type="List" optional="true"/>
        <attribute name="resultList" mode="OUT" type="List" optional="true"/>
    </service>

    <service name="standardImportExternalFileAsync" engine="simple" invoke="standardImportExternalFileAsync" location="com/mapsengineering/base/base-services.xml" auth="true" use-transaction="false" max-retry="0">
        <attribute name="sessionId" mode="OUT" type="String" optional="false"/>
    </service>
    
    <service name="standardImportExternalFile" engine="java" location="com.mapsengineering.base.standardimport.ImportManagerExternalFile" invoke="doImportSrv" auth="true" use-transaction="false" max-retry="0">
        <attribute name="checkOnlyMoveSmbFile" mode="IN" type="String" optional="true"/>
        <attribute name="checkOnlyUpload" mode="IN" type="String" optional="true"/>
        <attribute name="sessionId" mode="INOUT" type="String" optional="true"/>
        <!-- Se servizio schedulato, viene preso dal file di properties -->
        <attribute name="defaultOrganizationPartyId" mode="IN" type="String" optional="true"/>
        <attribute name="jobLogger" mode="INOUT" type="com.mapsengineering.base.util.JobLogger" optional="true"/>
        <attribute name="blockingErrors" mode="OUT" type="Long" optional="false"/>
        <attribute name="recordElaborated" mode="OUT" type="Long" optional="false"/>
        <attribute name="resultListUploadFile" mode="OUT" type="List" optional="true"/>
        <attribute name="resultETLList" mode="OUT" type="List" optional="true"/>
        <attribute name="resultList" mode="OUT" type="List" optional="true"/>
    </service>
    
