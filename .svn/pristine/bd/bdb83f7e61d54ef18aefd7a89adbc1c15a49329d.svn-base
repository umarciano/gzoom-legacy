<?xml version="1.0" encoding="UTF-8"?>

<simple-methods xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/simple-methods.xsd">

    <simple-method method-name="createNoteDataEvent" short-description="createNoteDataEvent">
        <log level="verbose" message="************************ createNoteDataEvent"></log>
        <make-value value-field="noteData" entity-name="NoteData"/>
        <set-pk-fields value-field="noteData" map="parameters"/>
        <set-nonpk-fields value-field="noteData" map="parameters"/>
        <if-empty field="noteData.noteDateTime">
            <now-timestamp field="noteData.noteDateTime"/>
        </if-empty>
        <if-empty field="noteData.noteId">
            <sequenced-id sequence-name="NoteData" field="noteData.noteId"/>
        </if-empty>
        <create-value value-field="noteData"/>
    </simple-method>

</simple-methods>
