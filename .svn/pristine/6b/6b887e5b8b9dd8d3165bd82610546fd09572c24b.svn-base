<?xml version="1.0" encoding="UTF-8"?>

<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

	<!-- Lookup Workeffort -->
	
    <form name="WorkEffortSearchForm" extends="BaseSearchForm" extends-resource="component://base/widget/BaseForms.xml" append-field="false">
        <actions>
            <set field="orderBy" value="workEffortId"/>
        </actions>
        <field name="workEffortId">
            <text-find ignore-case="true" hide-options="ignore-case"/>
        </field>
        <field name="workEffortName">
            <text-find ignore-case="true" hide-options="ignore-case" size="50"/>
        </field>
    </form>

    <form name="WorkEffortAdvancedSearchForm" extends="BaseAdvancedSearchForm" extends-resource="component://base/widget/BaseForms.xml">
    </form>

    <form name="WorkEffortSearchResultListForm" id="WEVSRL901_WorkEffortView" extends="BaseSearchResultListForm" extends-resource="component://base/widget/BaseForms.xml">
        <field name="workEffortId">
            <hidden/>
        </field>
        <field name="workEffortName" sort-field="true">
            <display/>
        </field>
        <field name="estimatedStartDate">
            <display type="date"/>
        </field>
        <field name="estimatedCompletionDate">
            <display type="date"/>
        </field>
    </form>
    
    
    <!-- Lookup WorkEffortMeasureView -->
    
    <form name="WorkEffortMeasureViewSearchForm" extends="BaseSearchForm" extends-resource="component://base/widget/BaseForms.xml">
        <field name="orderBy">
            <hidden value="weMeasureMeasureType|weMeasureAccountCode"/>
        </field>
    	<field name="weMeasureWeId">
    		<hidden/>
    	</field>
        <field name="glResourceTypeId">
            <hidden/>
        </field>
        <field name="accountTypeEnumId">
            <hidden/>
        </field>
    	<field name="weMeasureId">
    		<text-find ignore-case="true" hide-options="ignore-case"/>
    	</field>
    	<field name="weMeasureDesc">
    		<text-find ignore-case="true" hide-options="ignore-case"/>
    	</field>
    	<field name="weMeasureMeasureType">
    		<drop-down allow-empty="true" current="selected" type="drop-down">
                <entity-options entity-name="Enumeration" description="${description}" key-field-name="enumId">
                	<select-field field-name="enumId" display="false"/>
                	<select-field field-name="description" display="true"/>
                    <entity-constraint name="enumTypeId" value="WE_MEASURE_TYPE"/>
                    <entity-order-by field-name="sequenceId"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="weMeasureAccountId">
        	<text-find ignore-case="true" hide-options="ignore-case"/>
        </field>
        <field name="weMeasureAccountDesc">
        	<text-find ignore-case="true" hide-options="ignore-case"/>
        </field>
    </form>
    
    <form name="WorkEffortMeasureViewAdvancedSearchForm" extends="BaseAdvancedSearchForm" extends-resource="component://base/widget/BaseForms.xml">
    </form>
    
    <form name="WorkEffortMeasureViewSearchResultListForm" id="WEMVSRL001_WorkEffortMeasureView" extends="BaseSearchResultListForm" extends-resource="component://base/widget/BaseForms.xml">
    	<field name="weMeasureMeasureTypeDesc">
    		<hidden/>
    	</field>
    	<field name="weMeasureProductId">
    		<hidden/>
    	</field>
    	<field name="weMeasureEmpPosTypeId">
    		<hidden/>
    	</field>
    	<field name="weMeasurePartyId">
    		<hidden/>
    	</field>
    	<field name="weMeasurePartyCode">
            <hidden/>
        </field>
    	<field name="weMeasureRoleTypeDesc">
    		<hidden/>
    	</field>
    	<field name="weMeasureOtherGoalDesc">
    		<hidden/>
    	</field>
    	<field name="weMeasureUomType">
    		<hidden/>
    	</field>
    	<field name="weMeasureUomDescr">
    		<hidden/>
    	</field>
    	<field name="weMeasureTypeValueDesc">
    		<hidden/>
    	</field>
    	<field name="weMeasureTypeValueId">
    		<hidden/>
    	</field>
    	<field name="weMeasureOtherWorkEffortId">
    		<hidden/>
    	</field>
    	<field name="weMeasurePeriodTypeDesc">
    		<hidden/>
    	</field>
        <field name="weMeasureAccountCode">
            <hidden/>
        </field>
        <field name="weMeasureDefaultUomId">
            <hidden/>
        </field>
        <!-- Serve per il campo descrizione del glAccountId inWorkEffortTransactionViewManagementMultiForm di WorkEffortMeasure -->
        <field name="weMeasureProductDesc">
            <hidden value="${weMeasureAccountCode} - ${weMeasureAccountDesc}"/>
        </field>
    	<field name="weMeasureId">
    		<display/>
    	</field>
    	<field name="weMeasureDesc">
    		<display description="${weMeasureUomDescr}"/>
    	</field>
    	<field name="weMeasureMeasureType">
    		<display-entity entity-name="Enumeration" description="${description}" key-field-name="enumId"/>
    	</field>
    	<field name="weMeasureAccountId">
    		<display/>
    	</field>
    	<field name="weMeasureAccountDesc">
    		<display/>
    	</field>
    </form>    
    
    <form name="WorkEffortMeasureViewWithWorkEffortPurposeTypeIndSearchForm" extends="GlAccountSearchLookupForm" extends-resource="component://accountingext/widget/forms/GlAccountForms.xml">
        <actions>
            <set field="orderBy" value="glAccountTypeId"/>

            <!--<set field="productId" from-field="productId" default-value="${parameters.productId}"/>-->
            <!--<set field="productId" from-field="productId" default-value="${parameters.INDProductId}"/>-->
        </actions>
        <field name="workEffortTypeIdRes">
            <hidden/>
        </field>
        <field name="workEffortTypeIdInd">
            <hidden/>
        </field>
        <field name="contentIdInd">
            <hidden/>
        </field>
        <field name="workEffortPurposeTypeIdInd">
            <hidden/>
        </field>
    </form>
    
    <form name="WorkEffortMeasureViewWithWorkEffortPurposeTypeSearchForm" extends="WorkEffortMeasureViewSearchForm">
        <field name="workEffortTypeIdRes">
            <hidden/>
        </field>
        <field name="workEffortTypeIdInd">
            <hidden/>
        </field>
    </form>
    <form name="WorkEffortMeasureViewWithWorkEffortPurposeTypeSearchResultListForm" extends="WorkEffortMeasureViewSearchResultListForm">
    </form>
    
    <form name="WorkEffortMeasureSearchForm" extends="BaseSearchForm" extends-resource="component://base/widget/BaseForms.xml">
        <field name="glAccountId">
            <text-find ignore-case="true" hide-options="ignore-case"/>
        </field>
        <field name="productId">
            <text-find ignore-case="true" hide-options="ignore-case"/>
        </field>
    </form>
    
     <form name="WorkEffortMeasureSearchResultListForm" id="WEMSRL001_WorkEffortMeasure" extends="BaseSearchResultListForm" extends-resource="component://base/widget/BaseForms.xml">
        <field name="workEffortMeasureId">
            <hidden/>
        </field>
        <field name="comments">
            <hidden/>
        </field>
        <field name="glAccountId">
            <display/>
        </field>
        <field name="productId">
            <display/>
        </field>
     </form>
</forms>
