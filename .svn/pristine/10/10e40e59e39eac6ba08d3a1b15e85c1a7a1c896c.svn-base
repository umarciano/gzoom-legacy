        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/base/" invoke="checkEnableNewThrowReport.groovy"/>
        <response name="successNewReport" type="view" value="workEffortNewPrintBirt"/>
        <response name="success" type="view" value="workEffortPrintBirt"/>
        <response name="error" type="view" value="error"/>
