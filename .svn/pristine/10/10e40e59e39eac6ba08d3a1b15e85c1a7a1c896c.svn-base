<?xml version="1.0" encoding="UTF-8"?>



<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">

    <include location="component://partyext/webapp/partyext/WEB-INF/controller.xml"/>
    <include location="component://workeffortext/webapp/workeffortext/WEB-INF/controller.xml"/>
    
        
    <description>EmplPerf Component Site Configuration File</description>
    <owner></owner>


    <!-- Events to run on every request before security (chains exempt) -->
    <!--
    <preprocessor>
    </preprocessor>
    -->
    
    <!-- Request Mappings -->
    <request-map uri="search">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="checkEntityEPAdmin.groovy"/>
        <response name="succesStartSearch" type="request" value="startSearch"/>
        <response name="success" type="view" value="search"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="workEffortStartSearch">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="executePerformFindEPWorkEffortRootInqy.groovy"/>
        <response name="success" type="view" value="search"/>
        <response name="management" type="view" value="management"/>
        <response name="error" type="view" value="search"/>
    </request-map>
    
    <request-map uri="workEffortSearchResultContainerOnly">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="executePerformFindEPWorkEffortRootInqy.groovy"/>
        <response name="success" type="view" value="searchResultContainerOnly"/>
        <response name="export-search" type="view" value="searchResultContainerOnlyExport"/>
        <response name="management" type="view" value="managementContainerOnly"/>
    </request-map>
    
    <request-map uri="workEffortRootStartSearch">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="executePerformFindEPWorkEffortRoot.groovy"/>
        <response name="success" type="view" value="search"/>
        <response name="management" type="view" value="management"/>
        <response name="error" type="view" value="search"/>
    </request-map>
    
    <request-map uri="workEffortRootSearchResultContainerOnly">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="executePerformFindEPWorkEffortRoot.groovy"/>
        <response name="success" type="view" value="searchResultContainerOnly"/>
        <response name="export-search" type="view" value="searchResultContainerOnlyExport"/>
        <response name="massive-print-search" type="request" value="asyncJobMassiveWorkEffortPrintAndSendMail"/>
        <response name="management" type="view" value="managementContainerOnly"/>
    </request-map>
    
    <request-map uri="asyncJobMassiveWorkEffortPrintAndSendMail">
        <security https="true" auth="true"/>
        <event type="service" invoke="asyncJobMassiveWorkEffortPrintAndSendMail"/>
        <response name="success" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    
    <!-- Scheda Individuale da Modello -->
    <request-map uri="lookupPartyEvalView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupPartyEvalManagerView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalManagerView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupPartyEvalOrganizationOrgPermissionView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalOrganizationOrgPermissionView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupPartyEvalOrganizationView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalOrganizationView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupPartyEvalAllOrganizationOrgPermissionView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalAllOrganizationOrgPermissionView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupPartyEvalAllOrganizationView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupPartyEvalAllOrganizationView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="lookupTemplateView">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="lookupTemplateView"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="workEffortRootTree">
        <security https="true" auth="true"/>
        <event type="groovy" path="com/mapsengineering/emplperf/" invoke="putIdInContext.groovy"/>
        <response name="success" type="view" value="workEffortRootTree"/>
        <response name="error" type="view" value="workEffortRootTree"/>
    </request-map>
    
    <request-map uri="workEffortAssocExtViewFromRefresh">
        <security https="true" auth="true"/>
        <event type="service" invoke="createObiettiviCollegati"/>
        <response name="success" type="request" value="childManagementListContainerOnly"/>
        <response name="error" type="view" value="error"/>
    </request-map>

    <request-map uri="emplPerfInsertFromTemplateScreen">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="emplPerfInsertFromTemplateScreen"/>
        <response name="error" type="view" value="error"/>
    </request-map>

    <request-map uri="emplPerfInsertFromTemplateAsync">
        <security https="true" auth="true"/>
        <event type="service" invoke="emplPerfInsertFromTemplateAsync"/>
        <response name="success" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>
    
    <request-map uri="emplPerfUpdateFromPreviousScreen">
        <security https="true" auth="true"/>
        <response name="success" type="view" value="emplPerfUpdateFromPreviousScreen"/>
        <response name="error" type="view" value="error"/>
    </request-map>
    
    <request-map uri="executeEmplPerfUpdateFromPreviousAsync">
        <security auth="true" https="true"/>
        <event type="service" invoke="executeEmplPerfUpdateFromPreviousAsync"/>
        <response name="success" type="request" value="json"/>
        <response name="error" type="request" value="json"/>
    </request-map>

	<!-- Mappa di lancio del report per il momento individuale apre la sua finestra -->
