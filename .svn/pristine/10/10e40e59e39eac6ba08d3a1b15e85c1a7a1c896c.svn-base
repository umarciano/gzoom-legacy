    <!-- View Mappings -->
	<view-map name="search" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#SearchScreen"/>
    <view-map name="searchContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#SearchContainerOnlyScreen"/>
    <view-map name="searchSimpleContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#SearchSimpleContainerOnlyScreen"/>
    <view-map name="searchResultContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#SearchResultContainerOnlyScreen"/>
    <view-map name="searchResultContainerOnlyExport" type="screenpoihssf" page="component://emplperf/widget/screens/CommonScreens.xml#SearchResultContainerOnlyScreen" content-type="application/vnd.ms-excel" encoding="none"/>
    <view-map name="lookup" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#LookupScreen"/>
    <view-map name="management" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ManagementScreen"/>
    <view-map name="managementContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ManagementContainerOnlyScreen"/>
    <view-map name="subFolderManagementContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#SubFolderManagementContainerOnlyScreen"/>
    <view-map name="childManagementContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ChildManagementContainerOnlyScreen"/>
    <view-map name="childManagementListContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ChildManagementListContainerOnlyScreen"/>
    <view-map name="print" type="screenfop" page="component://emplperf/widget/screens/CommonScreens.xml#FoScreen" content-type="application/pdf" encoding="none"/>
    <view-map name="lookupPartyEvalView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalView"/>
    <view-map name="lookupPartyEvalManagerView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalManagerView"/>
    <view-map name="lookupPartyEvalOrganizationView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalOrganizationView"/>
    <view-map name="lookupPartyEvalOrganizationOrgPermissionView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalOrganizationOrgPermissionView"/>    
    <view-map name="lookupPartyEvalAllOrganizationView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalAllOrganizationView"/>
    <view-map name="lookupPartyEvalAllOrganizationOrgPermissionView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupPartyEvalAllOrganizationOrgPermissionView"/>
    
    <view-map name="lookupTemplateView" type="screen" page="component://emplperf/widget/screens/LookupScreens.xml#LookupTemplateView"/>
    <view-map name="showPortalPageContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ShowPortalPageContainerOnly"/>
    <view-map name="showPortletContainerOnly" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#ShowPortletContainerOnly"/>
    
    <view-map name="workEffortPrintBirt" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#EmplPerfPrintBirt"></view-map>
    <view-map name="workEffortAnalysisPrintBirt" type="screen" page="component://emplperf/widget/screens/CommonScreens.xml#WorkEffortAnalysisPrintBirt"></view-map>
    
    <view-map name="EditCommunicationEvent" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#EditCommunicationEvent"/>

    <view-map name="externalWorkEffortViewPage" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#ExternalWorkEffortViewPageScreen"></view-map>
    <view-map name="externalWorkEffortViewContainerOnly" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#ExternalWorkEffortViewContainerOnlyScreen"></view-map>
    <view-map name="externalSurveyPage" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#ExternalSurveyPageScreen"></view-map>
    <view-map name="externalSurveyContainerOnly" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#ExternalSurveyContainerOnlyScreen"></view-map>
    <view-map name="emplPerfInsertFromTemplateScreen" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#EmplPerfInsertFromTemplateScreen"/>
    <view-map name="emplPerfUpdateFromPreviousScreen" type="screen" page="component://emplperf/widget/screens/EmplPerfScreens.xml#EmplPerfUpdateFromPreviousScreen"/>
