                <!-- If javascript allowed -->
                <entity-options entity-name="GeoType" key-field-name="geoTypeId" description="${description}">
                    <select-field field-name="description" display="true"/>
                    <select-field field-name="geoTypeId"/>
                    <entity-order-by field-name="geoTypeId"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="isReserved" sort-field="true">
            <display description="${bsh: return uiLabelMap.get(&quot;Y&quot;.equals(context.get(&quot;isReserved&quot;)) ? &quot;CommonYes&quot; : &quot;CommonNo&quot;);}"/>
        </field>
        <field name="hasGeoRef" use-when="${bsh: return ((org.ofbiz.base.util.UtilValidate.isNotEmpty(context.get(&quot;isReserved&quot;)) &amp;&amp; &quot;Y&quot;.equals(context.get(&quot;isReserved&quot;))) ? true : false);}" sort-field="true">
            <display description="${bsh: return uiLabelMap.get(&quot;Y&quot;.equals(context.get(&quot;hasGeoRef&quot;)) ? &quot;CommonYes&quot; : &quot;CommonNo&quot;);}"/>
        </field>
        <field name="hasGeoRef" use-when="${bsh: return ((org.ofbiz.base.util.UtilValidate.isEmpty(context.get(&quot;isReserved&quot;)) || &quot;N&quot;.equals(context.get(&quot;isReserved&quot;))) ? true : false);}" sort-field="true">
