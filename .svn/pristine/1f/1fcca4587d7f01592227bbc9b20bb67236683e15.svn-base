                    <if-has-permission permission="COMMONEXT" action="_UPDATE"/>
                </or>
            </not>
        </read-only>
        <field name="geoTypeId" use-when="${bsh: return (org.ofbiz.base.util.UtilValidate.isNotEmpty(context.get(&quot;geoTypeId&quot;)) ? true : false);}" sort-field="true" title="${uiLabelMap.CommonId}">
