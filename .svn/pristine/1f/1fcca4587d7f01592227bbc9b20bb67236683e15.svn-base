        </field>
        <field name="parentTypeId" use-when="${bsh: return ((org.ofbiz.base.util.UtilValidate.isEmpty(context.get(&quot;isReserved&quot;)) || &quot;N&quot;.equals(context.get(&quot;isReserved&quot;))) ? true : false);}" title="${uiLabelMap.parentLabel}" sort-field="true">
