            <text size="50" maxlength="255"/>
        </field>
        <field name="parentTypeId" use-when="${bsh: return ((org.ofbiz.base.util.UtilValidate.isNotEmpty(context.get(&quot;isReserved&quot;)) &amp;&amp; &quot;Y&quot;.equals(context.get(&quot;isReserved&quot;))) ? true : false);}" title="${uiLabelMap.parentLabel}" sort-field="true">
