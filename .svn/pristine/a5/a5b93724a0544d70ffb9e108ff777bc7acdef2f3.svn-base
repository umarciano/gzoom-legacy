/* ********************************** */
/*             Menu                   */
/* ********************************** */
.menu-container, .menu-container ul {
    float: left;
}

.menu-container ul li {
    padding-top: 0 !important;
    padding-bottom: 0 !important;
    margin-left: 0.2em !important;
    margin-right: 0.2em !important;
    overflow: hidden;
    height: 17px;
    width: 22px;
    background-repeat: no-repeat;
