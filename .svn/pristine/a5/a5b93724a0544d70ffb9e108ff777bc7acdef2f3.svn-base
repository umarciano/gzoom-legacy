/* ********************************** */
/* Modifiche apportate agli stili     */
/* originari per renderli compatibili */
/* con le specifiche del documento di */
/* standard                           */
/* ********************************** */
html {
    margin: 0;
    padding: 0;
}

body {
    background-color: #FFFFFF;
}

html, body {
    height: 100%;
}

div.page-container {
    margin: auto;
    padding: 0 1em;
    min-height: 100%;
    position: relative;
}

div.page-container {
    _height: 98%;
    position: relative;
}

div.contentarea {
    margin:0;
    border: none;
    padding-bottom: 60px;
}

div.portlet {
    position: relative;
}

.multi-editable input {
    border: none;
}

.nav-pager {
    margin:15px auto;
    padding-top:0.2em;
    padding-bottom:0.3em;
    text-align:left;
    width:250px;
}

.fieldgroup-title-bar {
    border-top:none;
}

form div.fieldgroup-title-bar {
    display: none;
}

div.screenlet, table input, select {
    color: #232323;
}
input[type="text"], input[type="password"] {
    color: #232323;
}
div.tableContainer {
    height: 235px !important;
}
/***************************************/

#footer {
    position: absolute;
    bottom: 0;
    width: 99%;
    height: 100px;
}
#footer {
    _width: 100%;
    _height: 60px;
}

#column-container {
    padding: 0;
    overflow:hidden;
}

.label {
    /* Used for <input> element labels */
    text-align: left !important;
    padding-left: 0.7em !important;
}
.hidden-button {
    _filter: alpha(opacity=0); /* FF HACK */
}
html>body .hidden-button {
    display: none; /* IE HACK */
}

#masthead {
    float: left;
    width: 99%;
}

#masthead  ul {
    float: left;
    width:100%;
}

#masthead  ul li {
    padding-right : 0;
}

#masthead .bottom-link p {
    color: #000099;
    vertical-align: bottom;
}
/* ********************************** */

/* ********************************** */
/*          Toolbar Container         */
/* ********************************** */
.toolbar-container {
    float:left;
    width: 100%;
}
html>body .toolbar-container {
    width: 99%;
}
.toolbar-container ul {
    display: inline;
}
.toolbar-container ul li.logo-area {
    float: left;
    width: 86%;
}
.toolbar-container ul li.control-area {
    float: right;
    padding: 0.3em 0.7em;
}
/* ********************************** */

/* ********************************** */
/*          Common Container          */
/* ********************************** */

html>body #common-container {
    width: 99%;
}

/* ********************************** */
/*   Menu/Toolbar nello screenlet     */
/* ********************************** */
.screenlet ul li {
    float: left;
}
.screenlet ul li.expanded,.screenlet ul li.collapsed { /* Per riportare l'elemento di espansione dello screenlet alla posizione originale a destra */
    float: right;
    background-position: center center !important;
    background-repeat: no-repeat !important;
    _padding-left: 1em;
    _padding-right: 1em;
}
.screenlet ul li.expanded a, .screenlet ul li.collapsed a {
    width: auto !important;
    height: 1% !important;
    left: 0.5em !important;
}

#main-container {
    overflow: auto;
}

#main-container-screenlet .screenlet ul li.expanded, #main-container-screenlet .screenlet ul li.collapsed { /* IE FIX Per simulare il selettore first:child */
    height: 12px;
}
#main-container-screenlet * .screenlet ul li.expanded, #main-container-screenlet * .screenlet ul li.collapsed { /* IE FIX Per simulare il selettore first:child */
    height: 15px;
}

#main-container-screenlet div.screenlet-title-bar { /* IE FIX Per simulare il selettore first:child */
    background-color: #d9d9d9;
    background-image: none;
    padding-top: 0.6em;
    padding-bottom: 0.6em;
    padding-right: 0.6em;
    _padding-bottom: 1em;
}

#main-container-screenlet * div.screenlet-title-bar { /* IE FIX Per simulare il selettore first:child */
    background-color: #000099;
    background-image: url(/images/gradient_sub_uns.gif);
    background-repeat: repeat-x;
    border-bottom: none;
    height: 1%; /* IE fix */
    min-height: 1.5em;
    padding-top: 0 !important;
    padding-bottom: 0 !important;
    _padding-top: 0.1em !important;
    _padding-bottom: 0.3em !important;
}
#main-container-screenlet * div.screenlet-title-bar ul li { /* IE FIX Per simulare il selettore first:child */
    margin-left: 0 !important;
    margin-right: 0 !important;
    height: 1%;
    width: auto;
}
/* ********************************** */

/* ********************************** */
/*             Menu                   */
/* ********************************** */
#masthead ul li.control-area li {
    padding-bottom: 0.6em;
    background-repeat: no-repeat;
}
#masthead ul li.control-area li a {
    position: relative;
    float:left;
    border: 0;
    top: 0; left: 0;
    height :17px;
    color: transparent;
    width: 17px;
    _filter:alpha(opacity=0);
}
/* ********************************** */

/* ********************************** */
/* Toolbar info prima riga di testata */
/* ********************************** */
li.info {
    background-image: url("/old_theme_gplus/icon/16/ie/info.gif");
}
html>body li.info {
    background-image: url("/old_theme_gplus/icon/16/ff/info.png");
}
li.help {
    background-image: url("/old_theme_gplus/icon/16/ie/help.gif");
}
html>body li.help {
    background-image: url("/old_theme_gplus/icon/16/ff/help.png");
}
li.gplus {
    background-image: url("/old_theme_gplus/icon/gplus.png");
    width: 30px !important;
    padding-right: 0 !important;
}
html>body li.gplus {
    background-image: url("/old_theme_gplus/icon/gplus.png");
}
li.user-preferences {
    background-image: url("/old_theme_gplus/icon/24/ie/kuser.gif");
    width: 30px !important;
    padding-right: 0 !important;
}
html>body li.user-preferences {
    background-image: url("/old_theme_gplus/icon/24/ff/kuser.png");
}
/* ********************************** */

/* ********************************** */
/*        Container Principale        */
/* ********************************** */
#main-container-screenlet {
    border: none;
}

#main-container-screenlet .screenlet {
    border-left: none;
