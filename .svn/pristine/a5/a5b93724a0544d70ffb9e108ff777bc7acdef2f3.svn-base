    margin-left: 0 !important; /* IE HACK */
    margin-right: 0 !important;
    width: 22px;
}
html>body .menu-container ul li {
    display: block;
}
.menu-container ul li a {
    position: relative;
    float:left;
    border: 0;
    top: 0; left: 0;
    height :17px;
    color: transparent;
    width: 17px;
