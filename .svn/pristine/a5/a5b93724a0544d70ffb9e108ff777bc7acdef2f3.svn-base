}


/* ********************************** */
/*          treeview                  */
/* ********************************** */

/* ---------- display class ---------*/
.narrow {
    display: none;
}

/*  --------- part of screen ------ */

/* columns title label */
div.treeview-slice-title  {
    width: 100%;
    text-align: center;
    text-shadow: gray;
    font-size: large;
    font-weight: bold;
    color: navy;
}


/*  need for scrolling content */
div.treeview-global-scroll-container {
    overflow: auto;
}

/* main screen container */
div.treeview-global-container {
    display: table;
    table-layout: auto;
    width: 100%;
}

/* Per le definizioni degli screen vedi BaseScreens.xml */
/* Left slice */
div.treeview-left-screen {
    border: thin;
    border-style: solid;
    padding: 0.5em;
    display: table-cell;
    width: 100px;  /* set minimum dim, this setting with tabel-cell display makes screens correctly dimensioned */
}
div.treeview-tree-holder {
    position: relative; /* serve per posizionare bene immagine di attesa */
    /*display: none;  attivato da javascript dopo la costruzione dell'albero (vedi loadTreeView.js)*/
}

/* detail slice */
div.treeview-detail-screen {
    border: thin;
    border-style: solid;
    padding: 0em;
    display: table-cell;

}
div.treeview-management-holder {
    position: relative; /* serve per posizionare bene immagine di attesa */
    overflow: auto;
}

/* inner container of left slice */
div.treeview-container-tree {
    overflow: auto;
}

/* tree header */
th.treeview-header-toolbar {
    padding: 2px !important;
}

/* tree header image */
th.treeview-header-toolbar li.treeview-header-toolbar-splitter {
    display: block;
    cursor: pointer;
    background-image: url(/old_theme_gplus/icon/treeview/openclose.png);
    width: 16px;
    height: 16px;
}

th > span.treeview-search-field-label {
    padding-left: 1px !important;
}

/*
    --------- Tree styles ------
 */
div.treeview-container-tree table {
  border-width: 1px;
  /* border-collapse: collapse; */
  line-height: 1;
  border-color: #888;
  border-style: solid;
}

div.treeview-container-tree table caption {
  font-size: .9em;
  font-weight: bold;
}

div.treeview-container-tree table thead {
  background: #aaa url(/old_theme_gplus/icon/treeview/table-thead.png) repeat-x top left;
  font-size: .9em;
}

div.treeview-container-tree table tbody tr.moved {
    background-color: #C9C299;
    font-weight: bolder;
}

div.treeview-container-tree table thead tr th {
  border: 2px solid #777;
  font-weight: normal;
  padding: .3em 1.67em .1em 1.67em;
  text-align: left;
}

div.treeview-container-tree table tbody tr td {
  padding: .3em 1.5em;
}

div.treeview-container-tree table span {
  cursor: pointer;
  background-position: center left;
  background-repeat: no-repeat;
  padding: .2em 0 .2em 1.5em;
}

div.treeview-container-tree table span.file {
  /* background-image: url(/theme_gplus/icon/treeview/white_text.png); */
  background-image: url(/old_theme_gplus/icon/treeview/folder.png);
  white-space: nowrap;
}

div.treeview-container-tree table span.folder {
  background-image: url(/old_theme_gplus/icon/treeview/folder.png);
  white-space: nowrap;
}

.treeTable tr td .expander {
  background-position: left center;
  background-repeat: no-repeat;
  cursor: pointer;
  padding: 0;
  zoom: 1; /* IE7 Hack */
}

.treeTable tr.collapsed td .expander {
  background-image: url(/old_theme_gplus/icon/treeview/expand-dark.png);
}

.treeTable tr.expanded td .expander {
  background-image: url(/old_theme_gplus/icon/treeview/collapse-dark.png);
}

.treeTable tr.selected, .treeTable tr.accept {
  background-color: #A7C0EB;
  color: #fff;
}

.treeTable tr.collapsed.selected td .expander, .treeTable tr.collapsed.accept td .expander {
  background-image: url(/old_theme_gplus/icon/treeview/expand-light.png);
}

.treeTable tr.expanded.selected td .expander, .treeTable tr.expanded.accept td .expander {
  background-image: url(/old_theme_gplus/icon/treeview/collapse-light.png);
}

.treeTable .ui-draggable-dragging {
  color: #000;
  z-index: 1;
}

