/*          context menu              */
/* ********************************** */
.context-menu-container {
    margin-bottom: -6px;
}
.context-menu-container ul {
    clear:both;
    height:20px;
    margin:0 0 5px;
    padding:0;
}
.context-menu-container ul li {
    float:left;
    margin-right:7px;
    text-align:center;
    color: #000099;
}
.context-menu-container ul li a {
    float: left;
    color:#666666;
    height:2.0em;
    padding:0 6px;
    text-decoration: none;
}
.context-menu-container ul li a.active {
    background-color:#DDDDDD;
}
