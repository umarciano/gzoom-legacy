/* ********************************** */
div.autocomplete {
    width: auto;
    background-color: #b9c5dd;
    height: 150px;
    overflow: auto;
}
div.autocomplete ul li {
    float: none;
    font-size: 1em;
    font-family : inherit;
}
div.autocomplete ul li .separator {
    padding-left: 0.3em;
    padding-right: 0.3em;
}
/* ********************************** */

/* ********************************** */
/*               tabs                 */
/* ********************************** */
.tabs {
    margin-left: -0.1em;
}

.tabs ul {
    clear:both;
    height:20px;
    margin:0 0 5px;
    padding:0;
}
.tabs ul li {
    margin: 0;
    padding:0 0 0 1.9em;
    color: #000099;
}
.tabs ul li a {
    float: left;
    color:#FFFFFF;
    padding:0.5em 1.9em 0.9em 0;
    text-decoration: none;
}
.tabs ul li:hover, .tabs ul li:hover a {
    background-position:0 -130px;
    text-decoration: none;
}
.tabs ul li:hover a {
    background-position: 100% -130px;
}
.tabs ul li.active a {
    color: #282828;
}
/* ********************************** */

