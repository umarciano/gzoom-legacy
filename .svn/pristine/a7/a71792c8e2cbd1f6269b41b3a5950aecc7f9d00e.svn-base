                    <entity-constraint name="weContextId" value="CTX_EP"/>
                    <entity-constraint name="weIsTemplate" value="Y"/>
                    <entity-constraint name="weActivation" value="ACTSTATUS_ACTIVE"/>
                    <entity-constraint name="uvUserLoginId" env-name="userLogin.userLoginId"/>
                    <entity-constraint name="workEffortTypeId" lookup-for-field-name="templateTypeId" />
                    <entity-order-by field-name="workEffortName"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="estimatedStartDate" title="${uiLabelMap.WorkEffortRootCopy_startDate}" widget-style="mandatory options-field">
            <date-time type="date"/>
        </field>
        <field name="estimatedCompletionDate" title="${uiLabelMap.WorkEffortRootCopy_thruDate}" widget-style="mandatory options-field" required-field="true">
            <date-time type="date"/>
        </field>
        <field name="readDate" required-field="true" widget-style="mandatory options-field">
            <date-time type="date"/>
        </field>
        <field name="writeDate">
            <date-time type="date"/>
        </field>   
        <field name="updateAcctgTransAndEntry" widget-style="mandatory options-field">
            <drop-down no-current-selected-key="Y" maxlength="1" size="1" current="selected">
                <option key="N" description="${uiLabelMap.CommonNo}"/>
                <option key="Y" description="${uiLabelMap.CommonYes}"/>
            </drop-down>
        </field>
        <field name="updateWorkEffortPurposeType" widget-style="options-field">
            <drop-down type="drop-list" maxlength="255" size="50" local-autocompleter="false" drop-list-key-field="workEffortPurposeTypeId" drop-list-display-field="description">
                <entity-options entity-name="WorkEffortPurposeType" key-field-name="workEffortPurposeTypeId" description="${description}">
                    <select-field field-name="description" display="true"/>
                    <select-field field-name="workEffortPurposeTypeId"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="updateWorkEffortNote" widget-style="mandatory options-field">
            <drop-down no-current-selected-key="Y" maxlength="1" size="1" current="selected">
                <option key="N" description="${uiLabelMap.CommonNo}"/>
                <option key="Y" description="${uiLabelMap.CommonYes}"/>
            </drop-down>
        </field>
        <field name="updateWorkEffortAssoc" widget-style="mandatory options-field">
            <drop-down no-current-selected-key="Y" maxlength="1" size="1" current="selected">
                <option key="N" description="${uiLabelMap.CommonNo}"/>
                <option key="Y" description="${uiLabelMap.CommonYes}"/>
            </drop-down>
        </field>
        <field name="updateWorkEffortAssocType" widget-style="options-field">       
            <drop-down type="drop-list" local-autocompleter="true" maxlength="255" size="50" drop-list-key-field="workEffortAssocTypeId" drop-list-display-field="description"> 
            	<auto-complete choices="100"/>        
            	<list-options key-name="workEffortAssocTypeId" list-name="workEffortAssocTypeList" description="${description}" />
            </drop-down>
        </field>                
    </form>
    
