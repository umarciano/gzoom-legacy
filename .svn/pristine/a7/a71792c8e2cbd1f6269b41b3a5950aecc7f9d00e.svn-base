	                    <if-has-permission permission="EMPLPERFORG" action="_ADMIN"/>
	                    <if-has-permission permission="EMPLPERFROLE" action="_ADMIN"/>
                    </or>
            	</not>
            </or>
        </read-only>
    </form>
    
