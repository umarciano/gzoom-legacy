
#mainmenu-container {
    width: 100%;
    height: 100%;
}

.treemenu-head {
    border: 1px solid;
}

.treemenu-upper-toolbar {
    min-height: 32px;
    width: 100%;
    background-color: #D9D9D9;
}

.treemenu-module-table {
    height: 100%;
    width: 100%;
}

.treemenu-module-table td {
    cursor: pointer;
    background-repeat: no-repeat;
    background-position: center;
    width: 32px;
    height: 32px;
}

.treemenu-module-table td.disabled-module {
    cursor: default;
}

.treemenu-table {
    width: 100%;
    padding-right: 0.5em;
}

.treemenu-table .search-field {
    width: 100%;
    color: #ADADAD;
    font-style: italic;
}

/*
   assegnazione delle immagini ai moduli, la classe relativa é l'id del modulo
 */
/* Lascio attivi per compatibilità con chi non carica ile nuove voci --- DA TOGLIERE */
.treemenu-module-table td.GP_MENU_BASEDATA {
   background-image: url(/theme_gplus/icon/32/ff/normal/system_zoom.png);
}
.treemenu-module-table td.GP_MENU_BASEDATA:hover {
   background-image: url(/theme_gplus/icon/32/ff/hot/system_zoom.png);
}
.treemenu-module-table td.GP_MENU_PARTY {
   background-image: url(/theme_gplus/icon/32/ff/normal/customers_zoom.png);
}
.treemenu-module-table td.GP_MENU_PARTY:hover {
    background-image: url(/theme_gplus/icon/32/ff/hot/customers_zoom.png);
}
.treemenu-module-table td.GP_MENU_PERFMGM {
   background-image: url(/theme_gplus/icon/32/ff/normal/balance_zoom.png);
}
.treemenu-module-table td.GP_MENU_PERFMGM:hover {
   background-image: url(/theme_gplus/icon/32/ff/hot/balance_zoom.png);
}
.treemenu-module-table td.GP_MENU_CNTGES {
   background-image: url(/theme_gplus/icon/32/ff/normal/economics_zoom.png);
}
.treemenu-module-table td.GP_MENU_CNTGES:hover {
   background-image: url(/theme_gplus/icon/32/ff/hot/economics_zoom.png);
}
.treemenu-module-table td.GP_MENU_RENSOC {
   background-image: url(/theme_gplus/icon/32/ff/normal/customers_zoom.png);
}
.treemenu-module-table td.GP_MENU_RENSOC:hover {
   background-image: url(/theme_gplus/icon/32/ff/hot/customers_zoom.png);
}
