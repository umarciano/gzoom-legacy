.treemenu-search-toolbar {
    height: 25px;
    width: 100%;
    background-color: #D9D9D9;
}

.treemenu-separator-toolbar {
	height: 2px; 
	border-bottom: solid 1px #808080;
}

.treemenu-function-toolbar {
    height: 28px;
    width: 100%;
    background-color: #D9D9D9;
}

.treemenu-function-toolbar table.treemenu-function-table {
    width: 100%;
    height: 100%;;
}

.treemenu-function-toolbar table.treemenu-function-table td {
    background-repeat: no-repeat;
    background-position: center;
    width: 42px;
    cursor: pointer;
}
/*
   assegnazione delle immagini alle funzioni
 */ 
.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-openall a {
    text-decoration: none;
    color: #808080 !important;
}
.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-openall a:hover {
    color: #FFF !important;
}
.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-openall a:before {
    content: "\f151";
    font-size: 16px;
}

.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-closeall a {
    text-decoration: none;
    color: #808080 !important;
}
.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-closeall a:hover {
    color: #FFF !important;
}
.treemenu-function-toolbar table.treemenu-function-table td#mainmenu-function-closeall a:before {
    content: "\f150";
    font-size: 16px;
}

.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-clock a {
    text-decoration: none;
    color: #808080 !important;
}
.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-clock a:hover {
    color: #FFF !important;
}
.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-clock a:before {
    content: "\f1da";
    font-size: 16px;
}

.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-preferred a {
    text-decoration: none;
    color: #808080 !important;
}
.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-preferred a:hover {
    color: #FFF !important;
}
.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-preferred a:before {
    content: "\f005";
    font-size: 16px;
}

.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-home a {
    text-decoration: none;
