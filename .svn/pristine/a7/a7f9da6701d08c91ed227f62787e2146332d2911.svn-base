}
.treemenu-function-toolbar table.treemenu-function-table td.treemenu-function-console:hover {
    background-image: url(/theme_gplus/icon/24/ff/hot/run.png);
}

.treemenu-body {
    height: 100%;
    width: 100%;
}

.treemenu-body-slidearea-preferred,  .treemenu-body-slidearea-history {
    margin: 0;
    padding: .5em;
    min-height: 65px;
    border: 1px inset;
}

/* item cliccabili in preferred & history*/
.treemenu-item-facility {
  cursor: pointer;
  font-family: Arial;
  font-size: x-small;
  display: inline;
  padding-left: 0.2em;
}
.treemenu-item-facility-delete {
  display: inline;
  cursor: pointer;
  height: 100%;
  color: red;
  font-family: fantasy;
  /*background-image: url("/theme_gplus/icon/16/ff/normal/close_b.png");*/
}

.treemenu-body-links {
    overflow: auto;
    border: 1px solid;
}

/* override impostazioni albero standard (in style.css) */
.treemenu-outer-table {
    height: 100%;
    width: 100%;
    border: none !important;
}

.treemenu-outer-table tbody tr.menu-item span {
  white-space: nowrap;
  font-size: 0.9em;
}

.treemenu-outer-table tbody tr.menu-folder span {
  white-space: nowrap;
  font-size: 0.9em;
}
