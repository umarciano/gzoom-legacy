<?xml version="1.0" encoding="UTF-8"?>

<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

	<form name="PartyExtStandardImportForm" id="PSTD001_StandardImportUploadedFile"  type="upload" target-window="target_upload" default-table-style="single-editable" style="uploadFile basic-form cachable content-upload-form" target="executeStandardImportUploadFile" >
		<field name="defaultOrganizationPartyId">
		    <hidden/>
		</field>
		<field name="organizationInterface" title="${uiLabelMap.OrganizationInterface}" >
            <display/>
        </field>
        <field name="entityListToImportOrganizationInterface" widget-style="execute-field" position="2" title=" ">
            <check all-checked="false">
                <option key="OrganizationInterface" description=" "/>
            </check>
        </field>
        <field name="OrganizationInterfaceRefDate" widget-style="options-field" position="3" title=" "  widget-area-style="columnWidthClass15">
            <date-time type="date"/>
        </field>
        <field name="OrganizationInterfaceDataSourceId" widget-style="options-field" position="4" title=" " widget-area-style="columnWidthClass25">
            <drop-down type="drop-list" maxlength="255" size="30" local-autocompleter="true" drop-list-key-field="dataSourceId" drop-list-display-field="description">
                <entity-options entity-name="DataSource" key-field-name="dataSourceId" description="${description}">
                    <select-field field-name="dataSourceId"/>
                    <select-field field-name="description" display="true"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="OrganizationInterfaceUploadedFile" widget-area-style="columnWidthClass20" position="5" title=" ">
