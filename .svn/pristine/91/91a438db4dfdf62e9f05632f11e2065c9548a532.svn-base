    <form name="WorkEffortTypeSearchForm" extends="WorkEffortTypeSearchForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
        <actions>
            <set field="orderBy" value="workEffortTypeId"/>
        </actions>
        <field name="parentTypeId">
            <hidden value="CTX_PM"/>
        </field>
    </form>
    
    <form name="WorkEffortTypeAdvancedSearchForm" extends="WorkEffortTypeAdvancedSearchForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
