    <form name="WorkEffortTypeStatusViewManagementForm" extends="WorkEffortTypeStatusViewManagementForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
        <read-only>
            <not>
                <or>
                    <if-has-permission permission="PROJECTMGR" action="_CREATE"/>
