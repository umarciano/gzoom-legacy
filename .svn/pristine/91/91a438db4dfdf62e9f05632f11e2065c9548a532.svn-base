    <form name="WorkEffortTypeSearchResultListForm" id="WETSRL001OR_WorkEffortType" extends="WorkEffortTypeSearchResultListForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
        <field name="parentTypeId">
            <hidden/>
        </field>
    </form>
    
    <form name="WorkEffortTypeManagementForm" extends="WorkEffortTypeManagementForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
        <read-only>
            <not>
                <or>
                    <if-has-permission permission="PROJECTMGR" action="_CREATE"/>
                    <if-has-permission permission="PROJECTMGR" action="_UPDATE"/>
                    <if-has-permission permission="PROJECTMGR_ORG" action="_ADMIN"/>
                </or>
            </not>
        </read-only>
        <field name="parentTypeId" required-field="true">
            <drop-down type="drop-list" maxlength="255" size="50" drop-list-key-field="workEffortTypeId" drop-list-display-field="description" read-only="true">
                <entity-options entity-name="WorkEffortType" key-field-name="workEffortTypeId" description="${description}">
                </entity-options>
            </drop-down>
        </field>
        
    </form>
    
    <form name="WorkEffortTypeRootManagementForm" extends="WorkEffortTypeRootManagementForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
        <read-only>
