    <form name="WorkEffortTypeContentManagementMultiForm" id="WTCMM001_WorkEffortTypeContent" extends="BaseManagementMultiForm" extends-resource="component://base/widget/BaseForms.xml">
        <read-only>
            <not>
                <or>
                    <if-has-permission permission="PROJECTMGR" action="_CREATE"/>
                    <if-has-permission permission="PROJECTMGR" action="_UPDATE"/>
                    <if-has-permission permission="PROJECTMGR_ORG" action="_ADMIN"/>
                </or>
            </not>
        </read-only>
        <field name="workEffortTypeId" required-field="true">
            <hidden/>
        </field>
        <field name="weTypeContentTypeId">
            <hidden/>
        </field>
    </form>
    
    <form name="WorkEffortTypeFolderManagementMultiForm" id="WTFMM001_WorkEffortTypeContent" extends="WorkEffortTypeContentManagementMultiForm">
        <field name="contentId" title="${uiLabelMap.FormFieldTitle_folderContentId}" required-field="true">
            <drop-down type="drop-list" maxlength="255" size="50" local-autocompleter="false" drop-list-key-field="contentId" drop-list-display-field="description" read-only="!&quot;Y&quot;.equals(parameters.get(&quot;insertMode&quot;))">
                <entity-options entity-name="ContentAssocViewTo" key-field-name="contentId" description="${description}">
                    <select-field field-name="description" display="true"/>
                    <select-field field-name="contentId"/>
                    <entity-constraint name="contentIdStart" env-name="contentIdStart"/>
                    <entity-constraint name="caContentAssocTypeId" env-name="contentAssocTypeId"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="etch" use-when="${groovy: import org.ofbiz.base.util.UtilValidate; return UtilValidate.isEmpty(context.get(&quot;multiTypeLang&quot;)) || &quot;NONE&quot;.equals(context.get(&quot;multiTypeLang&quot;))}">
            <text size="50" maxlength="255"/>
        </field>
        <field name="etch" title="${wetfolderEtchTitle}"  use-when="${groovy: import org.ofbiz.base.util.UtilValidate; return UtilValidate.isNotEmpty(context.get(&quot;multiTypeLang&quot;)) &amp;&amp; !&quot;NONE&quot;.equals(context.get(&quot;multiTypeLang&quot;))}">
            <text size="50" maxlength="255"/>
        </field>        
        <field name="etchLang" title="${wetfolderEtchLangTitle}" use-when="${groovy: import org.ofbiz.base.util.UtilValidate; return UtilValidate.isNotEmpty(context.get(&quot;multiTypeLang&quot;)) &amp;&amp; !&quot;NONE&quot;.equals(context.get(&quot;multiTypeLang&quot;))}">
            <text size="50" maxlength="255"/>
        </field>
        <sort-order>
            <sort-field name="contentId"/>
            <sort-field name="etch"/>
            <sort-field name="etchLang"/>
        </sort-order>
    </form>
    <form name="WorkEffortTypePrintManagementMultiForm" id="WTPMM001_WorkEffortTypeContent" extends="WorkEffortTypeContentManagementMultiForm">
        <field name="contentId" title="${uiLabelMap.FormFieldTitle_printContentId}" required-field="true">
            <drop-down type="drop-list" maxlength="255" size="50" local-autocompleter="false" drop-list-key-field="contentId" drop-list-display-field="description" read-only="!&quot;Y&quot;.equals(parameters.get(&quot;insertMode&quot;))">
                <entity-options entity-name="ContentAssocViewTo" key-field-name="contentId" description="${description}">
                    <select-field field-name="description" display="true"/>
                    <select-field field-name="contentId"/>
                    <entity-constraint name="contentIdStart" operator="in" value="${contentIdStart}"/>
                    <entity-constraint name="caContentAssocTypeId" env-name="contentAssocTypeId"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="sequenceNum">
            <text size="50" maxlength="255"/>
        </field>
        <sort-order>
            <sort-field name="contentId"/>
            <sort-field name="sequenceNum"/>
        </sort-order>
    </form>
    
    <form name="WorkEffortTypeAttrAndNoteDataManagementForm"  extends="WorkEffortTypeAttrAndNoteDataManagementForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
    	<read-only>
            <not>
                <or>
                    <if-has-permission permission="PROJECTMGR" action="_CREATE"/>
                    <if-has-permission permission="PROJECTMGR" action="_UPDATE"/>
                    <if-has-permission permission="PROJECTMGR_ORG" action="_ADMIN"/>
                </or>
            </not>
        </read-only>
    </form> 
    
    <form name="WorkEffortTypeIndicatorManagementMultiForm" id="WTFMM001_WorkEffortTypeContentIndicator" extends="WorkEffortTypeIndicatorManagementMultiForm">
        
        <field name="contentId" title="${uiLabelMap.FormFieldTitle_folderContentId}" required-field="true">
            <drop-down type="drop-list" maxlength="255" size="50" local-autocompleter="false" drop-list-key-field="contentId" drop-list-display-field="description" read-only="!&quot;Y&quot;.equals(parameters.get(&quot;insertMode&quot;))">
                <entity-options entity-name="ContentAssocViewTo" key-field-name="contentId" description="${description}">
                    <select-field field-name="description" display="true"/>
                    <select-field field-name="contentId"/>
                    <entity-constraint name="contentIdStart" env-name="contentIdStart"/>
                    <entity-constraint name="caContentAssocTypeId" env-name="contentAssocTypeId"/>
                    <entity-constraint name="contentId" operator="like" value="WEFLD_IND%" />
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        <field name="workEffortPurposeTypeId" title="${uiLabelMap.WorkEffortPurposeType}" required-field="true">
            <drop-down type="drop-list" maxlength="255" size="50" local-autocompleter="false" drop-list-key-field="workEffortPurposeTypeId" drop-list-display-field="description" >
                <entity-options entity-name="WorkEffortPurposeType" key-field-name="workEffortPurposeTypeId" description="${description}">
                    <select-field field-name="workEffortPurposeTypeId" display="false"/>
                    <select-field field-name="description" display="true"/>
                    <entity-constraint name="purposeTypeEnumId" value="PT_INDICATOR"/>
                    <entity-order-by field-name="description"/>
                </entity-options>
            </drop-down>
        </field>
        
    </form> 
    
    <form name="WorkEffortTypeContentTypeManagementMultiForm" id="WETCT0001_WorkEffortTypeContentType" extends="WorkEffortTypeContentTypeManagementMultiForm" extends-resource="component://workeffortext/widget/forms/WorkEffortTypeForms.xml">
    </form>  
</forms>
