    <!-- Easter Monday to be continued -->

    <!-- A Mon-Fri expression that excludes Italian holidays -->
    <TemporalExpression tempExprId="MAPS_ITA_WORK_SCHED" tempExprTypeId="DIFFERENCE" description="Monday to Friday without Italian Holidays"/>
    <TemporalExpressionAssoc fromTempExprId="MAPS_ITA_WORK_SCHED" toTempExprId="MON_TO_FRI" exprAssocType="INCLUDE"/>
    <TemporalExpressionAssoc fromTempExprId="MAPS_ITA_WORK_SCHED" toTempExprId="ITA_HOLIDAYS" exprAssocType="EXCLUDE"/>

