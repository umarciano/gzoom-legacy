	    <parameter name="excludeIntroduction" default-value="N"/>
	    <parameter name="excludeExtensionDescriotion" default-value="N"/>	    
	    <parameter name="excludeValidity" default-value="N"/>
	    <parameter name="exposePaginator" default-value="Y"/>
	    <parameter name="exposeReleaseDate" default-value="Y"/>
	    <parameter name="typeNotes" default-value="ALL"/>
	    <parameter name="selectNote" default-value="DATE"/>
	    <parameter name="excludeIndicator" default-value="N"/>
	    <parameter name="exposeDetailIndicator" default-value="Y"/>
	    <parameter name="excludeScore" default-value="Y"/>
	    <parameter name="exposeOnlyIndex" default-value="N"/>
	    <parameter name="outputFormat"/>
	    <parameter name="reportContentId"/>
	    <parameter name="userProfile">
	    	<event type="java" path="com.mapsengineering.workeffortext.birt.event.WorkEffortextBirtEvents" invoke="setUserProfile"/>
	    </parameter>
    </report>
    
