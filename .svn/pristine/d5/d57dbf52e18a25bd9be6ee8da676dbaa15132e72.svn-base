<?xml version="1.0" encoding="UTF-8" ?>
<reports xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/report-parameters.xsd">
	
    <report id="REPORT_PROC" name="SchedaProcedimento" description="Scheda Procedimento">
	    <parameter name="workEffortId" />
	    <parameter name="workEffortIdChild">
           <event type="java" path="com.mapsengineering.workeffortext.birt.event.WorkEffortextBirtEvents" invoke="checkChildRootEquality"/>
        </parameter>
	    <parameter name="workEffortTypeId"/>
	    <parameter name="orgUnitId"/>
	    <parameter name="monitoringDate" type="Timestamp"  mandatory="true" validation-message="BaseErrorLabels#MonitoringDateNotNullValidation"/>
