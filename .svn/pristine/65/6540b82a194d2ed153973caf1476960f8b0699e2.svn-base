body, textarea, input, select {
    font-family: Trebuchet MS;
    font-size: 12px !important;
    color: #000000;
}

select {
    background-color: #FFFFFF;
    border: 0.1em solid #999999;
    font-size: 1.1em;
    height: 1.7em;
    margin: 0.1em;
}


a, a:visited {
    color: #0b0088;
    font-weight: bold;
}

/* treemenu */
.treemenu-head,
.mainmenu-function-table,
.treemenu-function-toolbar,
.treemenu-search-toolbar,
.treemenu-module-table {
    background-color: #B6DAFF !important;
    
    /*background-color: transparent !important;*/
}

table.treeTable tr.selected, .treeTable tr.accept {
    text-decoration: underline;
    color: #000000;
    background-color: #d2e6ef;
}

table.treeTable tr.collapsed.selected td .expander, .treeTable tr.collapsed.accept td .expander {
  background-image: url("/theme_gplus/icon/treeview/expand-dark.png");
}

.treeTable tr.expanded.selected td .expander, .treeTable tr.expanded.accept td .expander {
  background-image: url(/theme_gplus/icon/treeview/collapse-dark.png);
}

/* masthead */
#masthead {
    background-color: #86abde;
    color: #000000;
    font-weight: bold;
    height: 1%;
}

#masthead .application-title {
    color: #7F7F7F;
}

#masthead .bottom-link p {
    color: #000000;
    vertical-align: bottom;
}

#breadcrumbs p {
    color: #000000;
    display: inline;
    font-size: 1.1em;
    font-weight: bold;
}


/* content */
DIV.screenlet-body {
    background-color: #EDFBFF;
    height: auto !important;
    padding: 0.4em;
}

#main-container {
    background-color: #EDFBFF;
    overflow: auto;
    position: relative;
}

DIV.contentarea {
    background-color: #EDFBFF;
    border: 0.1em solid #000000;
    margin: 1em;
}

DIV.screenlet {
    background-color: #EDFBFF;
    border: 0.1em solid #999999;
    height: auto !important;
    margin-bottom: 1em;
}

/*toolbar and title */
#main-container-screenlet div.screenlet-title-bar {
    background-color: #B6DAFF;
    background-image: none;
    padding-bottom: 0.6em;
    padding-right: 0.6em;
    padding-top: 0.6em;
}

.basic-form table .viewManyTR1, .basic-form table .alternate-row {
    background-color: #EEEEEE;
}

form a.image_anchor {
    border: 1px solid;
    display: block;
    padding: 1em;
    width: 18px;
}

.basic-table .header-row-2 th, .basic-table .header-row-2 td {
    border-bottom: 0.1em solid #CCCCCC;
    border-left: 0.1em solid #CCCCCC;
    font-weight: bold;
    padding: 0.4em;
    text-align: center;
}

.basic-table .header-row-2 th, .basic-table .header-row-2 th a, .basic-table .header-row-2 td {
    color: #000000;
}


