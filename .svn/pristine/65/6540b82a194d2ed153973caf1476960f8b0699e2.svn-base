/* tab */


.tabs ul li {
    background:transparent url(/theme_gplus/images/blueacc/tab-selected-left-blue-bg.png) no-repeat scroll left top;
}
.tabs ul li a {
    background-color: #FFF !important;
    border-bottom: thin solid #B6DAFF;
    border-right: thin solid #6277e3;
    /*border-top: thin solid #FFFFFF;*/
    border-top: 1px solid #6277e3;
    
    color: #000;
    height: 11px !important;    
}
.tabs ul li:hover a {
    background-color:#cc1f00;
}
.tabs ul li:hover, .tabs ul li:hover a {
    background-position:0 0px !important;
}
.tabs ul li.active {
    background-color: #B6DAFF;
    border-bottom: thin solid #B6DAFF;
    background: none repeat scroll 0 0 transparent;
    
    /*padding-bottom: 0.4em !important;*/
    background:transparent url(/theme_gplus/images/blueacc/tab-left-blue-bg.png) no-repeat scroll left top;
}

.tabs ul li.active a {
    background-color: #B6DAFF !important;
    border-top: solid 1px #6277e3;
    border-bottom: thin solid #B6DAFF !important;
}



